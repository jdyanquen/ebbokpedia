<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:image field='gae' src='resources/powered-by-gae.png' />
	<ui:image field='gwt' src='resources/powered-by-gwt.png' />

	<ui:style>
		.wrap {
			clear: both;
			margin: 0;
			padding: 0;
			width: 977px;
			height: 77px;
		}
		
		@sprite .gae {
			float: left;
			gwt-image: 'gae';
			margin-top: 30px;
			margin-left: 30px;
		}
		
		@sprite .gwt {
			float: left;
			gwt-image: 'gwt';
			margin-top: 30px;
			margin-left: 30px;
		}
		
		.links a {
			float: right;
			padding: 20px 30px 0 0;
		}
	</ui:style>
	<g:HTMLPanel styleName='{style.wrap}'>
		<span class='{style.gae}' />
		<span class='{style.gwt}' />
		<span class='{style.links}'>
			<a href="#">About Us</a>
			<a href="#">Terms and Conditions</a>
		</span>
	</g:HTMLPanel>
</ui:UiBinder> 